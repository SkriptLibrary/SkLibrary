on load:
  set {config::prefix} to "<#5C27C9><b>S<#7730BB><b>k<#9239AD><b>L<#AD429E><b>i<#C84B90><b>b<#CE637F><b>r<#D47B6D><b>a<#D9925C><b>r<#DFAA4A><b>y<reset> <gray>Â»"
